# @name tdx_guest_bat_tests
# @desc Check tdx guest bat status
# @requires tdx
# @setup_requires
# @utility_requires

TDX_XS_BAT_GUEST_CPUINFO_NEW cd ddt_intel/lkvs/guest-test && ./guest.test_launcher.sh -v 1 -s 1 -m 1 -d on -t tdx -f tdx -x TD_GUEST_CPUINFO -c "accept_memory=lazy" -p off
TDX_XS_BAT_GUEST_KCONFIG_NEW cd ddt_intel/lkvs/guest-test && ./guest.test_launcher.sh -v 1 -s 1 -m 1 -d on -t tdx -f tdx -x TD_GUEST_KCONFIG -c "accept_memory=lazy" -p off
