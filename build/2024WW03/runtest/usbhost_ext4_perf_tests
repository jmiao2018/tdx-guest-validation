# @name USBHOST_SD-PERF-TESTS
# @desc USBHOST_SD-PERF-TESTS perform all performance Test Cases for ext4
# @requires usbhost && ext4
# @setup_requires
#EXT4 FS TC's
USBHOST_S_PERF_EXT4_2M blk_device_filesystem_perf_test.sh -f 'ext4' -s '2' -B '262144' -d 'usb' -c '1'
USBHOST_S_PERF_EXT4_2M_SYNC blk_device_filesystem_perf_test.sh -f 'ext4' -s '2' -o 'sync' -B '262144' -d 'usb' -c '1'
USBHOST_S_PERF_EXT4_4M blk_device_filesystem_perf_test.sh -f 'ext4' -s '4' -B '262144' -d 'usb' -c '1'
USBHOST_S_PERF_EXT4_8M blk_device_filesystem_perf_test.sh -f 'ext4' -s '8' -B '262144' -d 'usb' -c '1'
USBHOST_S_PERF_EXT4_10M blk_device_filesystem_perf_test.sh -f 'ext4' -s '10' -B '262144' -d 'usb'
USBHOST_S_PERF_EXT4_10M_BUFS blk_device_filesystem_perf_test.sh -f 'ext4' -s '10' -B '4096 8192 16384 32768 65536' -d 'usb'
USBHOST_S_PERF_EXT4_100M blk_device_filesystem_perf_test.sh -f 'ext4' -s '100' -B '262144' -d 'usb'
USBHOST_S_PERF_EXT4_100M_BUFS blk_device_filesystem_perf_test.sh -f 'ext4' -s '100' -B '102400 262144 524288 1048576 5242880' -d 'usb'
USBHOST_S_PERF_EXT4_100M_SYNC blk_device_filesystem_perf_test.sh -f 'ext4' -s '100' -o 'sync' -B '262144' -d 'usb'
USBHOST_S_PERF_EXT4_100M_SYNC_BUFS blk_device_filesystem_perf_test.sh -f 'ext4' -s '100' -o 'sync' -B '102400 262144 524288 1048576 5242880' -d 'usb'
