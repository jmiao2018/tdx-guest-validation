# @name SATA device w/r test with fs:vfat
# @desc Perform mkfs first, then do read write
#	The device node is specified in get_blk_device_node.sh
# @requires sata && vfat
# @setup_requires
# @utility_requires pvdisplay

SATA_S_FUNC_VFAT_DD_RW_1B blk_device_dd_readwrite_test.sh -f "vfat" -b "1" -c "1" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_512B blk_device_dd_readwrite_test.sh -f "vfat" -b "512" -c "1" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_64K blk_device_dd_readwrite_test.sh -f "vfat" -b "64K" -c "1" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_512K blk_device_dd_readwrite_test.sh -f "vfat" -b "512K" -c "1" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_5M blk_device_dd_readwrite_test.sh -f "vfat" -b "5M" -c "1" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_640K blk_device_dd_readwrite_test.sh -f "vfat" -b "64K" -c "10" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_10M blk_device_dd_readwrite_test.sh -f "vfat" -b "1M" -c "10" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_50M blk_device_dd_readwrite_test.sh -f "vfat" -b "5M" -c "10" -d "sata"

SATA_S_FUNC_VFAT_DD_RW_100M blk_device_dd_readwrite_test.sh -f "vfat" -b "10M" -c "10" -d "sata"

SATA_M_FUNC_VFAT_DD_RW_1G blk_device_dd_readwrite_test.sh -f "vfat" -b "10M" -c "100" -d "sata"

SATA_M_FUNC_VFAT_DD_RW_10G blk_device_dd_readwrite_test.sh -f "vfat" -b "1M" -c "1024" -l 10 -w -d "sata"
