# @name EMMC-PERF-TESTS
# @desc EMMC-PERF-TESTS perform all performance Test Cases for vfat
# @requires emmc && vfat
# @setup_requires

#VFAT FS TC's
EMMC_S_PERF_VFAT_2M blk_device_filesystem_perf_test.sh -f 'vfat' -s '2' -B '262144' -d 'emmc' -c '1'

EMMC_S_PERF_VFAT_2M_SYNC blk_device_filesystem_perf_test.sh -f 'vfat' -s '2' -o 'sync' -B '262144' -d 'emmc' -c '1'

EMMC_S_PERF_VFAT_4M blk_device_filesystem_perf_test.sh -f 'vfat' -s '4' -B '262144' -d 'emmc' -c '1'

EMMC_S_PERF_VFAT_8M blk_device_filesystem_perf_test.sh -f 'vfat' -s '8' -B '262144' -d 'emmc' -c '1'

EMMC_S_PERF_VFAT_10M blk_device_filesystem_perf_test.sh -f 'vfat' -s '10' -B '262144' -d 'emmc'

EMMC_S_PERF_VFAT_10M_BUFS blk_device_filesystem_perf_test.sh -f 'vfat' -s '10' -B '4096 8192 16384 32768 65536' -d 'emmc'

EMMC_S_PERF_VFAT_100M blk_device_filesystem_perf_test.sh -f 'vfat' -s '100' -B '262144' -d 'emmc'

EMMC_S_PERF_VFAT_100M_BUFS blk_device_filesystem_perf_test.sh -f 'vfat' -s '100' -B '102400 262144 524288 1048576 5242880' -d 'emmc'

EMMC_S_PERF_VFAT_100M_SYNC blk_device_filesystem_perf_test.sh -f 'vfat' -s '100' -o 'sync' -B '262144' -d 'emmc'

EMMC_S_PERF_VFAT_100M_SYNC_BUFS blk_device_filesystem_perf_test.sh -f 'vfat' -s '100' -o 'sync' -B '102400 262144 524288 1048576 5242880' -d 'emmc'
