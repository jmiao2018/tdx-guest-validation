# @name EMMC_PARTITION_FUNC_TESTS
# @desc EMMC_PARTITION_FUNC_TESTS contains tests for create, delete, mount,
#       umount partitions and to write/read data between partitions.
# @requires emmc
# @setup_requires

EMMC_XS_FUNC_CREATE_1P blk_device_create_erase_partition.sh -d "emmc" -p "1"

EMMC_XS_FUNC_FORMAT_1P_ALL_FS blk_device_readwrite_partitions.sh -d "emmc" -p "1" -f

EMMC_XS_FUNC_MOUNT_1P_ALL_MODES blk_device_readwrite_partitions.sh -d "emmc" -p "1" -f -m

EMMC_XS_FUNC_CREATE_2P blk_device_create_erase_partition.sh -d "emmc" -p "2"

EMMC_XS_FUNC_FORMAT_2P_ALL_FS blk_device_readwrite_partitions.sh -d "emmc" -p "2" -f

EMMC_XS_FUNC_MOUNT_2P_ALL_MODES blk_device_readwrite_partitions.sh -d "emmc" -p "2" -f -m

EMMC_XS_FUNC_RW_EXT2_2P blk_device_readwrite_partitions.sh -d "emmc" -p "2" -s "ext2" -w

EMMC_XS_FUNC_CREATE_3P blk_device_create_erase_partition.sh -d "emmc" -p "3"

EMMC_XS_FUNC_FORMAT_3P_ALL_FS blk_device_readwrite_partitions.sh -d "emmc" -p "3" -f

EMMC_XS_FUNC_MOUNT_3P_ALL_MODES blk_device_readwrite_partitions.sh -d "emmc" -p "3" -f -m

EMMC_XS_FUNC_RW_EXT3_3P blk_device_readwrite_partitions.sh -d "emmc" -p "3" -s "ext3" -w

EMMC_XS_FUNC_CREATE_4P blk_device_create_erase_partition.sh -d "emmc" -p "4"

EMMC_XS_FUNC_FORMAT_4P_ALL_FS blk_device_readwrite_partitions.sh -d "emmc" -p "4" -f

EMMC_XS_FUNC_MOUNT_4P_ALL_MODES blk_device_readwrite_partitions.sh -d "emmc" -p "4" -f -m

EMMC_XS_FUNC_RW_EXT4_4P blk_device_readwrite_partitions.sh -d "emmc" -p "4" -s "ext4" -w

EMMC_XS_FUNC_RW_VFAT_4P blk_device_readwrite_partitions.sh -d "emmc" -p "4" -s "vfat" -w
