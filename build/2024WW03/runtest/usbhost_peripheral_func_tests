# @name USBHOST_PERIPHERAL_FUNC_TESTS
# @desc usbhost different peripheral tests
# @requires usbhost && powermgr
# @setup_requires

# USBHOST DIFFERENT PERIPHERAL TEST
USBHOST_XS_FUNC_PRINTER usb_peripheral_tests.sh -m printer -p "03f0:df11" -n "DeskJet-1110-series"
USBHOST_XS_FUNC_PRINTER_FREE usb_peripheral_tests.sh -m printer -s freeze -p "03f0:df11" -n "DeskJet-1110-series"
USBHOST_XS_FUNC_PRINTER_MEM usb_peripheral_tests.sh -m printer -s mem -p "03f0:df11" -n "DeskJet-1110-series"
USBHOST_XS_FUNC_PRINTER_DISK usb_peripheral_tests.sh -m printer -s disk -p "03f0:df11" -n "DeskJet-1110-series"
