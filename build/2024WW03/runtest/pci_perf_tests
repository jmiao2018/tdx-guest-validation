# @name pci_perf_tests
# @desc Check pci basic driver performance
# @requires pci
# @utility_requires lspci
# @setup_requires

PCIe_PERF_ETH_IPERF_TCP_16K_76B pci_run_iperf.sh -m -M 88 -f M -d -t 30  -w 16K

PCIe_PERF_ETH_IPERF_TCP_64K_76B pci_run_iperf.sh -m -M 88 -f M -d -t 30 -w 64K

PCIe_PERF_ETH_IPERF_TCP_256K_76B pci_run_iperf.sh -m -M 88 -f M -d -t 30 -w 256K

PCIe_PERF_ETH_IPERF_TCP_16K_200B pci_run_iperf.sh -m -M 212 -f M -d -t 30 -w 16K

PCIe_PERF_ETH_IPERF_TCP_64K_200B pci_run_iperf.sh -m -M 212 -f M -d -t 30 -w 64K

PCIe_PERF_ETH_IPERF_TCP_256K_200B pci_run_iperf.sh -m -M 212 -f M -d -t 30 -w 256K

PCIe_PERF_ETH_IPERF_TCP_16K_1448B pci_run_iperf.sh -m -f M -d -t 30 -w 16K

PCIe_PERF_ETH_IPERF_TCP_64K_1448B pci_run_iperf.sh -m -f M -d -t 30 -w 64K

PCIe_PERF_ETH_IPERF_TCP_256K_1448B pci_run_iperf.sh -m -f M -d -t 30 -w 256K
