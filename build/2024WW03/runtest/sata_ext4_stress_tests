# @name SATA device w/r stress test with fs:ext4 and size:10M (bs=10M;count=1)
# @desc Perform mkfs first, then do read write
#	The device node is specified in get_blk_device_node.sh
# @requires sata && ext4
# @setup_requires sata

SATA_L_STRESS_EXT4_DD_RW_100M blk_device_dd_readwrite_test.sh -f "ext4" -b "10M" -c "10" -d "sata" -l "300"

SATA_XL_STRESS_EXT4_DD_RW_1G blk_device_dd_readwrite_test.sh -f "ext4" -b "10M" -c "100" -d "sata" -l "100"
